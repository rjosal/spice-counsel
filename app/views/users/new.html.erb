<div style="text-align:center; padding: 20px;">
<%= image_tag 'logo_alternative.png' %>
</div>
<div id="howtocounsel" style="width:330px;margin:0 auto 0 auto;">
<h1>Sign Up</h1>

<% form_for(@user) do |f| %>
  <%= f.error_messages %>

  <table> 
  <tr>
    <td><%= f.label :name %></td>
    <td><%= f.text_field :name %></td>
  </tr><tr>
    <td><%= f.label :email %></td>
    <td><%= f.text_field :email %></td>
  </tr><tr>
    <td><%= f.label :password %></td>
    <td><%= f.password_field :password, :value => '' %></td>
  </tr><tr>
    <td colspan="2"><%= f.submit 'Create' %></td>
  </tr>
  </table>
<% end %>
</div
<%= link_to 'Back', '/home/login' %>
